<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="25fit_place_into_bar" inherit_id="website.snippets" name="25fit place into bar">
        <xpath expr="//div[@id='snippet_custom']" position="before">
            <div id="25fit_snippet" class="o_panel">
                <div class="o_panel_header">25FIT Snippet</div>
                <div class="o_panel_body">
                    <t t-snippet="theme_25fit.s_antiseptic_model" t-thumbnail="/theme_25fit/static/description/snippets_thumbs/s_thumbnail_snippet_25fit.png">
                        <keywords>Antiseptic Model</keywords>
                    </t>
                </div>
            </div>
        </xpath>
    </template>
</odoo>